/**index.wxss**/

.image_swiper{
  width:100%;
  height: auto;
}
.image_swiper{
  width:100%;
}


.imageBanner {
  width: 100%;
  height: 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
 
.imageBanner_small {
  transform: scale(0.93);
  transition: 0.2s, ease-in 0s;
  border-radius: 10rpx;
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: -16rpx;
  z-index: 100;
  padding: 0;
  line-height: 100%;
  background: transparent;
  text-align: left;
}
 
